{
  "name": "ichwilldich-lib",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "sync": "svelte-kit sync",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./shadcn/*": "./src/lib/components/ui/*/index.ts",
    "./shadcn/css": "./src/app.css",
    "./backend": "./src/lib/backend/index.ts",
    "./components/form": "./src/lib/components/form/index.ts",
    "./components/nav": "./src/lib/components/nav/index.ts",
    "./components/table": "./src/lib/components/table/index.ts",
    "./components/util": "./src/lib/components/util/index.ts",
    "./hooks": "./src/lib/hooks/index.ts",
    "./util": "./src/lib/util/index.ts",
    "./utils": "./src/lib/utils.ts"
  },
  "peerDependencies": {
    "svelte": "5.38.7"
  },
  "dependencies": {
    "@internationalized/date": "3.9.0",
    "@lucide/svelte": "0.544.0",
    "@sveltejs/kit": "2.37.1",
    "@sveltejs/vite-plugin-svelte": "6.1.4",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.13",
    "@tanstack/table-core": "8.21.3",
    "@types/luxon": "3.7.1",
    "bits-ui": "2.9.6",
    "clsx": "2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "2.0.1",
    "layerchart": "^2.0.0-next.38",
    "luxon": "3.7.2",
    "mode-watcher": "^1.0.8",
    "paneforge": "^1.0.0-next.5",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "2.27.1",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0",
    "tailwindcss": "4.1.13",
    "tw-animate-css": "^1.3.0",
    "vaul-svelte": "^1.0.0-next.7",
    "zod": "4.0.10"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/package": "2.5.0",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "publint": "0.3.12",
    "svelte": "5.38.7",
    "svelte-check": "4.3.1",
    "typescript": "5.9.2",
    "vite": "7.1.5"
  }
}