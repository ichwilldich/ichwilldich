{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "groupName": "{{manager}}",
      "matchPackagePatterns": [
        "*"
      ]
    }
  ],
  "semanticCommitType": "chore",
  "automerge": true,
  "automergeStrategy": "merge-commit"
}